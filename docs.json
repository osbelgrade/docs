{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "analytics": {
    "customScripts": [
      {
        "src": "https://v0-osb-agent.vercel.app/zora-embed.js",
        "async": true
      }
    ]
  },
  "feeback": {
    "suggestEdit": false,
    "raiseIssue": false
  },
  "name": "COS: Belgrade",
  "colors": {
    "primary": "#ff4b33",
    "light": "#ff4b33",
    "dark": "#ff4b33"
  },
  "styling": {
    "eyebrows": "breadcrumbs"
  },
  "modeToggle": {
    "default": "dark",
    "strict": "false"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "COS:Belgrade",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["index", "quickstart", "development"]
          },
          {
            "group": "Chapters",
            "pages": [
              "chapters/core-functions",
              "chapters/user-interface",
              "chapters/background-processes",
              "chapters/system-errors",
              "chapters/version-history"
            ]
          },
          {
            "group": "Codex",
            "pages": ["essentials/images", "essentials/audio", "essentials/snippets", "essentials/downloads"]
          },
          {
            "group": "System",
            "pages": [
              "changelog",
              "ai-tools/zora",
              "ai-tools/branding",
              "ai-tools/share",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          },
          {
            "group": "Admin",
            "pages": ["about", "tools", "wwtc", "reticulate"]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/osbelgradelogo.svg",
    "dark": "/logo/osbelgradelogo-dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:david@reticulate.blog"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://osbelgrade.mintlify.app"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude", "perplexity", "mcp", "cursor", "vscode"],
    "feedback": {
      "suggestEdit": false,
      "raiseIssue": false
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
  }
}